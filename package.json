  "scripts": {
    "w-dev:prod": "SET NODE_ENV=production& nodemon server/server.js",
    "w-dev:dev": "SET NODE_ENV=development& nodemon server/server.js",
    "dev:prod": "NODE_ENV=production& nodemon server/server.js",
    "dev:dev": "NODE_ENV=development& nodemon server/server.js",
    "init-su-role": "node server/initialize/makeRole.js",
    "init-db": "node server/initialize/makeDatabase.js",
    "init-ext": "node server/initialize/makeExtensions.js",
    "schema": "node server/setup/psqlSchema.js",
    "roles": "node server/setup/psqlRoles.js",
    "auth": "node server/setup/psqlAuth.js",
    "functions": "node server/setup/psqlFunctions.js",
    "perms": "node server/setup/psqlPerms.js",
    "init": "npm-run-all init-su-role init-db init-ext",
    "setup": "npm-run-all schema roles auth functions perms",
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 8080",
    "first-time-setup": "npm-run-all init setup",
    "pm2": "pm2 start ecosystem.config.js --env prod && pm2 start npm --name \"next\" -- start && pm2 save"
  },
